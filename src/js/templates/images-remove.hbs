{{#if actions}}
	<div class="medium-insert-images-toolbar2 medium-editor-toolbar medium-editor-toolbar-active">
		<ul class="medium-editor-toolbar-actions clearfix">
        	{{#each actions}}
        	    {{#if label}}
        	        <li>
        	            <button class="medium-editor-action" data-action="{{@key}}">{{{label}}}</button>
        	        </li>
        	    {{/if}}
        	{{/each}}
    	</ul>
    </div>
{{/if}}